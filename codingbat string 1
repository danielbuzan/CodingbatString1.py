# Codingbat String 1
def hello_name(name):
  return "Hello {}!".format (name)

def make_abba(a, b):
   return a + b + b + a


def make_tags(tag, word):
  return ("<{}>".format(tag)) + ("{}".format(word)) + ("</{}>".format(tag))

def make_out_word(out, word):
  return out[0: 2] + word + out[2: 4]

def extra_end(str):
  return str[-2:] * 3


def first_two(str):
  return str[0:2]



def without_end(str):
  return str[1: -1]



def non_start(a, b):
  return str(a)[1:] + str(b)[1:]
